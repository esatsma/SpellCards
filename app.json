{
  "expo": {
    "name": "SpellCards",
    "slug": "spellcards",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.latsma.spellcards"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.latsma.spellcards"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "4fe39282-3790-4084-91ed-3e66c01c7074"
      }
    },
    "owner": "latsma"
  }
}
